<script>
    var srpImageMetrics = (function () {
      var pageId, srpImages, periodicMeasure,
          constructMetricParamString,
          registerImage,
          setImageSource,
          setPageId;
      pageId = "unknown";
      srpImages = {};
      periodicMeasure = setInterval(function () {
          var shouldSendResult = true,
              key;
          shouldSendResult = true;
          for (key in srpImages) {
              if (srpImages[key] === "") {
                  shouldSendResult = false;
                  break;
              }
          }
          if (shouldSendResult) {
              (new Image()).src = "/timer.gif?imgmtr=" + constructMetricParamString();
              clearInterval(periodicMeasure);
          }
      }, 1500);
      constructMetricParamString = function () {
          var params = pageId + ";",
              key;
          for (key in srpImages) {
              params += key + "=" + srpImages[key] + "&";
          }
          if (params.endsWith("&")) {
              params = params.substring(0, params.length-1);
          }
          return params;
      };
      setPageId = function(currentPageId) {
        pageId = currentPageId;
      };
      registerImage = function (resultNumber) {
          srpImages["r" + resultNumber] = "";
      };
      setImageSource = function (resultNumber, source) {
          srpImages["r" + resultNumber] = source;
      };
      return {
          SELLER_IMAGE: "si",
          IMAGE_SERVICE: "is",
          TITE: "ti",
          LIBRI: "li",
          ITALIAN: "it",
          OTHER: "ot",
          PAAPI: "pa",
          NO_IMAGE: "no",
          NO_IMAGE_NO_ISBN: "no_no",
          registerImage: registerImage,
          setImageSource: setImageSource,
          setPageId: setPageId
      };
  }());
  srpImageMetrics.setPageId("searchResults");
</script>

