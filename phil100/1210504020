		<script type="text/javascript">
			var startTime;
			var endTime;
			$(window).unload(function() {
				if (jwplayer().getState() == 'PLAYING') {
					console.log(jwplayer().getState());
					var theDate = new Date();
					var oneYearLater = new Date( theDate.getTime() + 31536000000 );
					var expiryDate = oneYearLater.toGMTString();
					document.cookie='2570421171210504020=' + jwplayer().getPosition() + '; expires=' + expiryDate  + 'domain=podbay.fm;path=/;';
				}
			});
			function startTime() {
			    var dc = document.cookie;
			    var cname = "2570421171210504020=";
			    if (dc.length > 0) {
			      begin = dc.indexOf(cname);
			      if (begin != -1) {
			        begin += cname.length;
			        end = dc.indexOf(";",begin);
			        if (end == -1) end = dc.length;
			        return unescape(dc.substring(begin, end));
			        }
			      }
			    return 0;
			}
			function secondstotime(secs)
			{
			    var t = new Date(1970,0,1);
			    t.setSeconds(secs);
			    var s = t.toTimeString().substr(0,8);
			    if(secs > 86399)
			        s = Math.floor((t - Date.parse("1/1/70")) / 3600000) + s.substr(2);
			    return s;
			 }
			function clipper () {
				if (document.getElementById('shareClip').innerHTML == '<h5>Cancel</h5>') {
					document.getElementById('shareClip').innerHTML = '<h5>Share a clip (Beta)</h5>';
					document.getElementById('sharer').style.display = 'none';
					window.resizeTo(320,520);
					jwplayer().resize(320,24);
				} else {
					document.getElementById('shareClip').innerHTML = '<h5>Cancel</h5>';
					window.resizeTo(640,520);
					jwplayer().resize(640,24);
					document.getElementById('sharer').style.display = 'inline';
				}
			}
			function saveStartTime() {
				if ((jwplayer().getPosition() < endTime) || (endTime==null)) {
					startTime = jwplayer().getPosition();
					document.getElementById('startTime').innerHTML = secondstotime(startTime);
					document.getElementById('startButton').innerHTML = 'Update Start Time';
					if ((startTime != null) && (endTime != null)) {
						document.getElementById('createClipBtn').style.display = 'inline';
					}
				}
			}
			function saveEndTime() {
				if (jwplayer().getPosition() > startTime) {
					endTime = jwplayer().getPosition();
					document.getElementById('endTime').innerHTML = secondstotime(endTime);
					document.getElementById('endButton').innerHTML = 'Update End Time';
					if ((startTime != null) && (endTime != null)) {
						document.getElementById('createClipBtn').style.display = 'inline-block';
					}
				}
			}
		</script>

