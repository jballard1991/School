			function popup(url) {
				if (jwplayer()) {
					var theDate = new Date();
					var oneYearLater = new Date( theDate.getTime() + 31536000000 );
					var expiryDate = oneYearLater.toGMTString();
					document.cookie='2570421171210504020=' + jwplayer().getPosition() + '; expires=' + expiryDate + 'domain=podbay.fm;path=/;';
					jwplayer().pause(true);
				}
newwindow=window.open(url,'name','toolbar=0,location=0,statusbar=0,menubar=0,resizable=0,height=510,width=320');
				if (window.focus) {newwindow.focus()}
				return false;
			}
			function popupvideo(url) {
newwindow=window.open(url,'name','toolbar=0,location=0,statusbar=0,menubar=0,resizable=0,height=480,width=640');
				if (window.focus) {newwindow.focus()}
				return false;
			}
