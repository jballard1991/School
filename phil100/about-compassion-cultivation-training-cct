<script type="text/javascript">

jQuery(document).ready(function() {

jQuery(".li7").before("<div class='cn-divider'></div>");

jQuery(".li7").after("<span>:</span>");

jQuery('li.li7').find('a[href*="http://ccare.stanford.edu/cct-directory-test?cn-s=&cn-cat=1"]').addClass('Uncategorized');

jQuery(".contry:contains( california )").text("");





jQuery( ".cn-divider " ).prev( "li" ).addClass('lastli');

jQuery('.contry').each(function(){

jQuery(this).text(jQuery(this).text().replace(/california|other-us-states|other-countries/g,""))

});



 jQuery('.li7 a[href="http://ccare.stanford.edu/cct-directory-test?cn-s=&cn-cat=7"]').each(function(){

 jQuery(this).text(jQuery(this).text().replace(/.U/g,"U"))

 });

 jQuery('.contry').each(function(){

 jQuery(this).text(jQuery(this).text().replace(/-/g," "))

 });



});



</script>

<script type="text/javascript">

jQuery(document).ready(function () {

    if(window.location.href.indexOf("?cn-s") > -1) {

        jQuery("#cn-list-body").css("display","block"); 

        jQuery(".desc").css("display","none"); 

    }



     if(window.location.href.indexOf("cct-directory-test/name/") > -1) {

        jQuery("#cn-list-body").css("display","block"); 

        jQuery(".desc").css("display","none"); 

         jQuery(".read-more").css("display","none");

        jQuery(".desc").remove();

        jQuery(".hd-text").remove();

        jQuery("#cn-list-body").css("border","none");

    }

    

     

});



</script>

