<script>
  (function () {
    // Must support JSON
    if (!(
      window.JSON &&
      JSON.stringify &&
      JSON.parse
    )) {
      return;
    }
  
    // Must support local storage
    var testVal = 'ted-abaptest';
    try {
      localStorage.setItem(testVal, testVal);
      localStorage.removeItem(testVal);
    } catch(e) {
      return;
    }
  
    // Build test list
    var tests = [
      // 0: Video background test
      {
        percent: 100,
        qualify: function () {
          try {
            if (
              null &&
              window.matchMedia('(min-width:1024px)').matches &&
  
              !(('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) &&
              !window.matchMedia('(-moz-touch-enabled)').matches
            ) {
              var elem = document.createElement('video');
              if (!elem.canPlayType('video/mp4; codecs="avc1.42E01E"').replace(/^no$/, '')) {
                return false;
              }
  
              return true;
            }
          } catch (e) {
            return false;
          }
        },
  
        B: function () {
          document.getElementById('player-hero-poster-video').innerHTML = '<video class="player-hero__poster__video" autoplay loop muted src="' + null + '">';
  
          document.getElementById('player-hero-poster').className += ' player-hero__poster--video';
        }
      },
  
      // 1: Autoplay - no design change
      {
        percent: 5,
        qualify: function () { return true; },
  
        A: function () {
          runAutoplayTest(1, 'A');
        },
        B: function () {
          runAutoplayTest(1, 'B');
        }
      },
  
      // 2: Autoplay - design change
      {
        percent: 5,
        qualify: function () { return true; },
  
        A: function () {
          runAutoplayTest(2, 'A');
        },
        B: function () {
          runAutoplayTest(2, 'B');
        }
      },
  
      // 3: Hover-to-play
      {
        percent: 20,
        qualify: function () {
          return  (
            window.matchMedia('(min-width:1024px)').matches &&
  
            !(('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) &&
            !window.matchMedia('(-moz-touch-enabled)').matches
          );
        },
  
        B: function () {
          document
            .getElementById('player-hero-play')
            .addEventListener('mouseover', function () {
              playerHeroPlay();
            });
  
          document
            .getElementById('player-hero')
            .className += ' player-hero--hover-to-play';
        }
      },
  
      // 4: Preroll autoplay
      {
        percent: 30,
        qualify: function () {
          try {
            if (
              93 > 1604 &&
              window.matchMedia('(min-width:1024px)').matches &&
  
              !(('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) &&
              !window.matchMedia('(-moz-touch-enabled)').matches
            ) {
              var elem = document.createElement('video');
              if (!elem.canPlayType('video/mp4; codecs="avc1.42E01E"').replace(/^no$/, '')) {
                return false;
              }
  
              return true;
            }
          } catch (e) {
            return false;
          }
        },
  
        B: function () {
          var video = document.createElement('video'),
              hasOverlayingClass;
          video.autoplay = true;
          video.muted = true;
          video.src = 'http://video.ted.com/videos/test/motion-hero/preroll.mp4';
          video.className = 'player-hero__preroll__video';
          video.addEventListener('timeupdate', function () {
            var currentTime = video.currentTime - 0.25,
                destinyTime = window.TED.playerOffset || 0;
  
            if (!destinyTime || destinyTime < currentTime) {
              window.TED.playerOffset = currentTime;
            }
  
            if (currentTime > 4.5 && !hasOverlayingClass) {
              hero.className += ' player-hero--preroll-overlaying';
              hasOverlayingClass = true;
            }
          });
  
          document.getElementById('player-hero-preroll-video').appendChild(video);
  
          var hero = document.getElementById('player-hero');
          hero.className += ' player-hero--preroll-overlay';
          video.addEventListener('ended', function () {
            hero.className += ' player-hero--preroll-overlaid';
  
            window.TED.abapGaSent10 = true;
            __ga(
              'send',
              'event',
              'player',
              'progressTime',
              '10 seconds'
            );
          });
  
          var videoPaused;
          function stopVideo() {
            if (!videoPaused) {
              video.pause();
              videoPaused = true;
            }
          }
  
          var clicker = document.getElementById('player-hero-play');
          clicker.addEventListener('touchend', stopVideo);
          clicker.addEventListener('click', stopVideo);
  
          window.TED.abapGaSentStart = true;
          __ga(
            'send',
            'event',
            'player.talk',
            'start',
            93
          );
        }
      }
    ];
  
    var lsKey = 'abap';
    localStorage.removeItem(lsKey);
    localStorage.removeItem(lsKey + '-1');
  
    function runTest(index, group) {
      var test = tests[index];
  
      if (test[group] && test.qualify()) {
        test[group]();
      }
    }
  
    function runAutoplayTest(treatmentNo, group) {
      window.disengageAutoplay = disableTests
      window.TED.autoplayExperimentNo = treatmentNo;
  
      var isAutoplay = group == 'B';
  
      document.documentElement.className += ' ' +
        lsKey + ' ' +
        lsKey + '--' + 'treatment-' + treatmentNo + ' ' +
        lsKey + '--' + 'autoplay-' + (
          isAutoplay ?
          'on' :
          'off'
        );
  
      if (isAutoplay) {
        document.getElementById('player-hero').className += ' player-hero--autoplay-yes';
      } else {
        document.getElementById('player-hero').className += ' player-hero--autoplay-no';
      }
    }
  
    // Handle forced experiments
    var hrefArray = location.href.split('#ABAP'),
        apskip = hrefArray.pop();
    if (apskip.match(/^[0-9][AB]$/)) {
      runTest(+apskip[0], apskip[1]);
      if (window.history && window.history.replaceState) {
        window.history.replaceState(null, null, hrefArray[0]);
      }
    }
  }());
</script>

