<script>
  $("#sortby-topbar, #sortby-bottombar").change(function () {
    $(this.form).submit();
  });

  
  $(".int-tip").popover({
    animation: true,
    container: "body",
            trigger: "hover",
        content: "An international edition is a textbook that has been published outside the U.S. International editions are often cheaper than the U.S. version. Customers located in the U.S. can now purchase international edition textbooks. However, the publishers of international editions generally do not authorize the sale and distribution of international editions in Canada, and such sale or distribution may violate the copyrights and trademarks of the publishers of such works.",
    title: "International Edition"
  });

  $(".pod-tip").popover({
    animation: true,
    container: "body",
            trigger: "hover",
        content: "This book is printed on demand.  It is a new book and will be printed once your order is received.",
    title: "Print on Demand"
  });

  if((parseInt($('#refinements').height(), 10) + 200) < parseInt($('.result-set').height(), 10)) {

    var tt = (this.top = parseInt(($('#refinements').offset().top - 10), 10));
    var bb = (this.bottom = parseInt(($(document).height() - $('#bottombar').offset().top + 25), 10));
    $('#refinements').affix({
      offset: {
        top: function () {
          return tt;
        },
        bottom: function () {
          return bb;
        }
      }
    })

    var sizeCheck;
    var refContainer = $('#refinements');
    if ($(window).height() <  refContainer.height()) {
      $(this).addClass('affix-static');
    }
    $(window).on("resize", function () {
      clearTimeout(sizeCheck);
      sizeCheck = setTimeout(function () {
        ($(window).height() < refContainer.height()) ? refContainer.addClass('affix-static') : refContainer.removeClass('affix-static');
      }, 100);
    });

    $('.active-location-label').on("mouseenter", function() {
      if (($(".location-list").offset().top + $(".location-list").height()) > ($(window).height()  + $(window).scrollTop())) {
       $(".location-list").css({
           bottom: "-15px",
           top: "auto"
       }).on("mouseleave", function(){
           $(this).css({
               bottom: "auto",
               top: "0"
           })
       });
      }
    });

    $('#refinements').on('affixed.bs.affix', function () {
      if ($(this).offset().top > $('#bottombar').offset().top) {
        $(this).data('bs.affix').checkPosition()
      }
    });
  }

  $('.search-term').inlineEdit({
    minCopy: "min",
    maxCopy: "max",
    toCopy: "to",
    ieSubmit:"Submit",
    ieClick:"Click to edit",
    ieDelete: "Delete",
    errorMsg:"Please enter at least one search term."
  });
</script>
