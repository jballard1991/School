    <script type="text/javascript">



jQuery(document).ready(function() {     

jQuery(".alltag").hover(function(){

jQuery(".termbox").show();

jQuery(".termbox").hover(function(){

jQuery(this).show();

});



jQuery(".termbox").mouseleave(function(){

jQuery(this).hide();

});

});



jQuery(".alltag").mouseleave(function(){

jQuery(".termbox").hide();

});

});

</script>



