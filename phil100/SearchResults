<script type="text/javascript">
  if(ResponsiveHelper.isMobileAgent){
    if(ResponsiveHelper.fullSiteCookie) {
      ResponsiveHelper.initToggle();
    }else{
      ResponsiveHelper.limitResultsPerPage();
    }
  }

  window.addEventListener("orientationchange", function() {
    ResponsiveHelper.resizeToggle()
  }, false);

  window.addEventListener("resize", function () {
    var toggleEls,
        i;

    if (document.getElementsByClassName) {
      toggleEls = document.getElementsByClassName("toggle");
      for (i = 0; i < toggleEls.length; i++) {
        toggleEls[i].removeAttribute("style");
      }
    }
  }, false);
</script>



